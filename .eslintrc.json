{
    "parser": "@typescript-eslint/parser",
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:@hfe/mrnbaseutils/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "czf"
    ],
    "rules": {
        "czf/all-text-need-font-if-one-has": 2,
        "czf/navigation-header-not-null": 2,
        "czf/border-value-should-be-integer": 2
    }
}
